#单曲播放页相关元素

playbtn: "com.shinyv.cnr:id/play_img" #播放按钮
nextbtn: "com.shinyv.cnr:id/play_song_next" #下一首
previousbtn: "com.shinyv.cnr:id/play_song_previous"  #上一首
playlist: "com.shinyv.cnr:id/play_song_list" #播放列表入口
alarm: "com.shinyv.cnr: id / play_song_alarm" #定时关闭
speed: "com.shinyv.cnr:id/ibtn_speed" #倍速
hd: "com.shinyv.cnr:id/ibtn_hd" #音质
rate: "com.shinyv.cnr:id/ibtn_rate" #音质右侧的按钮
more: "com.shinyv.cnr:id/play_song_more" #右上角更多按钮
songback: "com.shinyv.cnr:id/play_song_back" #返回按钮
albumtitle: "com.shinyv.cnr:id/tv_title" #专辑名称
playsongname: "com.shinyv.cnr:id/play_song_name"  #单曲名称
playseekbar: "com.shinyv.cnr:id/play_song_seekbar" #播放进度条
orderbtn: "com.shinyv.cnr:id/btn_order" #订阅按钮
songlist: "com.shinyv.cnr:id/song_list_listview" #播放列表
playmode: "com.shinyv.cnr:id/tv_play_mode" #播放模式：顺序，随机，单曲循环
songlistsort: "com.shinyv.cnr:id/tv_sort" #播放列表排序
songlist_closedbtn: "com.shinyv.cnr:id/song_list_close" #播放列表关闭按钮